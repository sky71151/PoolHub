#ifndef INC_ESP_H_
#define INC_ESP_H_


typedef enum{
	disconnected = 0u,
	connected,
}WifiMqttState;

struct Time{
	char Hour;
	char Minutes;
};

typedef struct Esp
{
	char WiFi;
	char Mqtt;
	struct Time Time;
}ESP_t;


#endif /* INC_ESP_H_ */
